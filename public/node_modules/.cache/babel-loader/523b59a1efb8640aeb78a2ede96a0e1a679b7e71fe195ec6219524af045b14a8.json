 console.log(`el: ${el}, i: ${i}`);\n      // `title: (!(${title}) == '' ? ${title}: '') , this.title: ${this.title}`\n      console.log(`el: ${el}`);\n      console.log(`title: ${el.title}`);\n      console.log(`user: ${el.user}`);\n      console.log(`published: ${el.published}`);\n\n      this.title = el.title;\n      this.user = el.user;\n    },\n  },\n};\n</script>\n\n<style>\n.list-item {\n  width: 100%;\n  background: aquamarine; /*#fcddaf; #dfbc4c*/\n  padding: 10px 15px;\n  margin-top: 10px;\n  border: 1px solid silver;\n  border-radius: 5px;\n  font-size: 14px;\n}\n\n.list-item p {\n  margin-bottom: 8px;\n}\n\n.list-item p span {\n  float: right;\n  font-size: 12px;\n  color: #8f772d;\n}\n\n.list-item p:last-child {\n  color: #b1953c;\n  font-size: 11px;\n}\n\n.list-item button {\n  /* display: block; */\n  float: right;\n  border: 0;\n  border-radius: 5px;\n  padding: 4px 8px;\n  margin: 0;\n  margin-left: 10px;\n  color: #fddaae;\n  cursor: pointer;\n  font-size: 12px;\n  /* transition: transform 300ms ease; */\n}\n\n.btnEdit {\n  background: #348531;\n}\n\n/* .list-item button:hover {\n  transform: scale(1.1);\n} */\n</style>\n"],"mappings":";gCAGMA,mBAAA,CAAkB,YAAd,WAAS;gCAYbA,mBAAA,CAAmB,YAAf,YAAU;;;;uBAdlBC,mBAAA,CA4BO,eA3BLD,mBAAA,CAWM,cAVJE,UAAkB,EAClBF,mBAAA,CAQO,e,gBAPLA,mBAAA,CAIE;IAHAG,IAAI,EAAC,MAAM;+DACFC,KAAA,CAAAC,KAAK,GAAAC,MAAA;IACdC,WAAW,EAAC;iDADHH,KAAA,CAAAC,KAAK,E,mBAGhBL,mBAAA,CAAkE;IAA3DG,IAAI,EAAC,MAAM;+DAAUC,KAAA,CAAAI,IAAI,GAAAF,MAAA;IAAEC,WAAW,EAAC;iDAAlBH,KAAA,CAAAI,IAAI,E,GAChCR,mBAAA,CAAuE;IAA/DG,IAAI,EAAC,QAAQ;IAACM,KAAK,EAAC,QAAQ;IAAEC,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAC,OAAA,IAAAD,QAAA,CAAAC,OAAA,IAAAF,IAAA,CAAO;KAAE,UAAQ,E,KAGlEZ,mBAAA,CAcM,cAbJe,UAAmB,EACnBC,mBAAA,uEAA0E,G,kBAC1Ef,mBAAA,CAUMgB,SAAA,QAAAC,WAAA,CAVmCd,KAAA,CAAAe,KAAK,GAAfC,EAAE,EAAEC,CAAC;yBAApCpB,mBAAA,CAUM;MAVDQ,KAAK,EAAC,WAAW;MAA2Ba,GAAG,EAAEF,EAAE,CAACG;QACvDvB,mBAAA,CAEI,Y,kCADCoB,EAAE,CAACf,KAAK,IAAG,GAAC,iBAAAL,mBAAA,CAA0B,cAAAwB,g